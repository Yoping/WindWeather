<set xmlns:android="http://schemas.android.com/apk/res/android">
    <rotate
        android:duration="800"
        android:fromDegrees="180"
        android:pivotX="0"
        android:pivotY="0"
        android:toDegrees="0"></rotate>


    <rotate
        android:startOffset="600"
        android:duration="800"
        android:fromDegrees="0"
