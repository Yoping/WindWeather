<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android">
    <rotate
        android:pivotY="50%"
        android:pivotX="50%"
        android:fromDegrees="0"
        android:toDegrees="5"></rotate>
    <rotate
        android:duration="200"
        android:startOffset="200"
        android:pivotY="50%"
        android:pivotX="50%"
        android:fromDegrees="0"
        android:toDegrees="-10"></rotate>
    <rotate
        android:duration="200"
        android:startOffset="400"
        android:pivotY="50%"
        android:pivotX="50%"
        android:fromDegrees="0"
        android:toDegrees="10"></rotate>
    <rotate
        android:duration="200"
        android:startOffset="600"
        android:pivotY="50%"
        android:pivotX="50%"
        android:fromDegrees="180"
        android:toDegrees="-10"></rotate>

    <rotate
        android:duration="200"
        android:startOffset="800"
        android:pivotY="50%"
        android:pivotX="50%"
        android:fromDegrees="180"
        android:toDegrees="10"></rotate>

</set>